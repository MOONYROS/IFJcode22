<?php
declare(strict_types=1);

$a = 1>0;
$b = 1<0;
$c = 1>1;
//$d = 1>=0;
//$e = 1>=1;
$f = 1<=1;
$g = 1<=0;
//$h = 1>=null;
//$i = 0<=null;
//$j = 0>null;
$k = 1!==1;
$l = 1!==0;
$m = 1===1;
$n = 1===0;
$o = 1!==null;
$p = 0!==null;
$q = 1===null;
$r = 0===null;
$s = null===0;
$t = null!==0;
$u = null===1;
$v = null!==1;

write(
$a, " 1>0\n", 
$b, " 1<0\n", 
$c, " 1>1\n",
//$d, " 1>=0\n",
//$e, " 1>=1\n",
$f, " 1<=1\n",
$g, " 1<=0\n",
//$h, " 1>=null\n",
//$i, " 0<=null\n",
//$j, " 0>null\n",
$k, " 1!==1\n",
$l, " 1!==0\n",
$m, " 1===1\n",
$n, " 1===0\n",
$o, " 1!==null\n",
$p, " 0!==null\n",
$q, " 1===null\n",
$r, " 0===null\n",
$s, " null===0\n",
$t, " null!==0\n",
$u, " null===1\n",
$v, " null!==1\n"
);