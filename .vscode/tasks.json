{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "main.c",
                "lex.c",
                "parser.c",
                "support.c",
                "expression.c",
                "tstack.c",
                "symtable.c",
                "token.c",
                "generator.c",
                "expStack.c",
                "-o",
                "gigachad_compiler"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}